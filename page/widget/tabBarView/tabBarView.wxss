/* page/widget/tabBarView/tabBarView.wxss */

.b_view{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 112rpx;
  z-index: 1000;
  opacity: 1;
  background: #fff;
  overflow: hidden;
}
.tabBar {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 140rpx;
  display: flex;
  flex-flow: row nowrap;
  z-index: 1001;
  background: rgba(0, 0, 0, 0);
  overflow: hidden;
}


.tabBar  .tab {
  width: 33.3%;
  display: flex;
  flex-flow: column;
  justify-content: center;
  align-items: center;
  background: #FFF;
}

.tabBar  .tab1 {
  margin-top: 28rpx;
}
.tabBar  .tab2 {
  background: rgba(0, 0, 0, 0);
  
}


